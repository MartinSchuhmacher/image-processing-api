{
   "name": "image-processing-api",
   "version": "1.0.0",
   "description": "Image processing API using TypeScript",
   "main": "dist/index.js",
   "scripts": {
      "build": "rm -rf dist && tsc",
      "dev": "tsc -w & nodemon .",
      "eslint": "eslint '*/**/*.{js,ts}'",
      "jasmine": "jasmine",
      "prettier": "prettier --find-config-path --write .",
      "start": "node dist/index.js",
      "test": "npm run build && npm run jasmine"
   },
   "keywords": [
      "typescript",
      "eslint",
      "prettier",
      "api"
   ],
   "author": "Martin Schuhmacher",
   "license": "ISC",
   "dependencies": {
      "express": "^4.17.1",
      "image-size": "^1.0.0",
      "jasmine": "^3.7.0",
      "jasmine-spec-reporter": "^7.0.0",
      "sharp": "^0.28.3",
      "supertest": "^6.1.3"
   },
   "devDependencies": {
      "@types/express": "^4.17.12",
      "@types/image-size": "^0.8.0",
      "@types/jasmine": "^3.7.7",
      "@types/node": "^15.12.1",
      "@types/sharp": "^0.28.3",
      "@types/supertest": "^2.0.11",
      "@typescript-eslint/eslint-plugin": "^4.26.0",
      "@typescript-eslint/parser": "^4.26.0",
      "eslint": "^7.27.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-prettier": "^3.4.0",
      "nodemon": "^2.0.7",
      "prettier": "^2.3.0",
      "typescript": "^4.3.2"
   }
}
